            <w:r>
              <w:rPr>
                <w:lang w:val="en-US" />
              </w:rPr>
              <w:t xml:space="preserve">What is </w:t>
            </w:r>
            <w:r w:rsidRPr="002B51C9">
              <w:rPr>
