            <w:r w:rsidRPr="003C3B35">
              <w:rPr>
                <w:lang w:val="en-US" />
              </w:rPr>
              <w:t xml:space="preserve">helps us understand how people make choices </w:t>
            </w:r>
            <w:r>
              <w:rPr>
                <w:lang w:val="en-US" />
              </w:rPr>
              <w:t xml:space="preserve">in the face of </w:t>
