                <w:lang w:val="en-US" />
              </w:rPr>
              <w:t>scarcity</w:t>
            </w:r>
            <w:r>
              <w:rPr>
                <w:lang w:val="en-US" />
              </w:rPr>
              <w:t>.</w:t>
